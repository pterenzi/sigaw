<p id="notice"><%= notice %></p>

<br>
<table class="table">
<tr>
<th>CÓDIGO</th>
<th>DESCRIÇÃO</th>
<th>AÇÃO</th>
</tr>

<% @historical_cashes.each do |historical_cash| %>
  <tr>
    <td width="10%" align='center'><%=h (historical_cash.id) %></td>
    <td width="80%" align='left'><%=h (historical_cash.description) %></td>
  <td width="10%" align='center'>   
    <%= tooltip link_to(image_tag("edit.png", {:border => 0}), edit_historical_cash_path(historical_cash)), { :tooltip => 'Editar Registro.' } %>&nbsp
    <%= tooltip link_to(image_tag("delete.png", {:border => 0}), historical_cash, method: :delete, data: { confirm: 'Confirma a exclusão?' }), { :tooltip => 'Deletar Registro.' }  %></td>  
  </tr>
<% end %>
</table>
<br>
<p><%= will_paginate @historical_cashes, :params => {:campo => params[:campo], :chave => params[:chave]} %></p>
